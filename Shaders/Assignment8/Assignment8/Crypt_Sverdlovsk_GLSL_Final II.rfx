    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D Texture;
uniform sampler2D Depth;
uniform float TurboFy;

uniform sampler2D textureForReport;

varying vec2 texCoord;
uniform float dofArea;

void main(void)
{
   vec3 kernel[15];
   kernel[0] = vec3( 0.0, 0.0, 0.1995);
   kernel[1] = vec3( 0.0, 1.0, 0.1761);
   kernel[2] = vec3( 0.0, 2.0, 0.121);
   kernel[3] = vec3( 0.0, 3.0, 0.0648);
   kernel[4] = vec3( 0.0, 4.0, 0.027);
   kernel[5] = vec3( 0.0, 5.0, 0.0088);
   kernel[6] = vec3( 0.0, 6.0, 0.0022);
   kernel[7] = vec3( 0.0, 7.0, 0.0004);
   kernel[8] = vec3( 0.0,-1.0, 0.1761);
   kernel[9] = vec3( 0.0,-2.0, 0.121);
   kernel[10]= vec3( 0.0,-3.0, 0.0648);
   kernel[11]= vec3( 0.0,-4.0, 0.027);
   kernel[12]= vec3( 0.0,-5.0, 0.0088);
   kernel[13]= vec3( 0.0,-6.0, 0.0022);
   kernel[14]= vec3( 0.0,-7.0, 0.0004);
   
   vec3 tmp=vec3(0.0,0.0,0.0);
   
   
   //Radial Blur // Turbo-fy
   vec2 center = vec2(0.5,0.5);
   vec2 texCenter = texCoord - center;
   float depth=texture2D(Depth, texCoord).y;
   for( int i=0; i<15; i++ ){    
      tmp += texture2D(Texture, center + texCenter+(depth*kernel[i].xy* dofArea)).xyz * kernel[i].z;
      texCenter+=texCenter*TurboFy;
   }

   
   //Crosshair generation
   vec4 crosshair = vec4(0.0,0.0,0.0,0.0);
   
   crosshair.x  = step(0.498, texCoord.x) - step(0.503, texCoord.x);
   crosshair.z  = step(0.55,  texCoord.x) - step(0.45,  texCoord.x);   
   crosshair.y  = step(0.498, texCoord.y) - step(0.502, texCoord.y); 
   crosshair.w  = step(0.55,  texCoord.y) - step(0.45,  texCoord.y);    
   crosshair.y *=  crosshair.z;
   crosshair.x *=  crosshair.w;
   
   tmp.x -= crosshair.x + crosshair.y;
   //end of crosshair generation
   
   vec4 tempusColorus = texture2D(textureForReport, texCoord);
   gl_FragColor = vec4(tmp,1.0);//tempusColorus;//
}]]></RmShaderCode></RmGLShader>
