    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec3 vLightVec;
varying vec3 vEyeVec;
varying vec3 threedee;
uniform float dFreq;
uniform float depthMul;
uniform float fogTimer;
uniform float fogSpeed;

uniform float LightSize;

uniform sampler3D Texture1;

// Do not forget to add encode function (like in shadow assignment)


void main(void)
{
    
   float Depth=length(vEyeVec)*0.1;
   
   float dTexture = texture3D(Texture1,threedee*dFreq + fogTimer*fogSpeed).x; 
   
   float LightDist=1.0-(length(vLightVec)*LightSize);
                                                                                                                     
   gl_FragColor = vec4( dTexture,Depth,LightDist, 1.0 );
                    
}]]></RmShaderCode></RmGLShader>
