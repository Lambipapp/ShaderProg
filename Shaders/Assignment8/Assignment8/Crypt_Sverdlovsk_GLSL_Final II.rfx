    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D Diffuse;
uniform sampler2D Lightmap;
uniform sampler2D Normalmap;
uniform sampler2D Lightvec;
uniform sampler2D Depth;
uniform float fogIntensifier;
uniform float sepiaSat;
varying vec2 texCoord;
uniform float contrast;
uniform float brightnes;

void main(void)
{

   //Luminance Weights kommer att anvndas fr Sepia
   vec3  luminanceWeights = vec3(0.299,0.587,0.114);  

   vec4 lightMapCol = texture2D(Lightmap, texCoord);
   vec4 depthPass = texture2D(Depth,texCoord);
   vec4 tex = texture2D(Diffuse,texCoord);
   vec3 N = texture2D(Normalmap,texCoord).xyz;
   vec3 L = texture2D(Lightvec,texCoord).xyz;
   
   N=(N-0.5)*2.0;
   L=(L-0.5)*2.0;
   float diffDot = dot(N, L);
   
   vec4 outCol=vec4(0.0);
   
   //Sammanslagning av pass
   outCol.xyz=tex.xyz*diffDot*tex.w*depthPass.z; 
   outCol.xyz+=lightMapCol.xyz;
   outCol +=depthPass.x * fogIntensifier;
   
   //Sepia, Contrast och Brightness
   float sepiaLum = dot(outCol.xyz, luminanceWeights);
   outCol.xyz = mix(vec3(sepiaLum*1.2,sepiaLum*1.0,sepiaLum*0.8),outCol.xyz, sepiaSat);
   
   outCol.xyz = mix(vec3(0.5,0.5,0.5),outCol.xyz, contrast)*brightnes;
   gl_FragColor = outCol;
}]]></RmShaderCode></RmGLShader>
